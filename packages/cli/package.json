{
    "name": "feelfreeai-cli",
    "version": "0.5.0",
    "description": "AI 기반 코딩 CLI - llama.cpp와 Gemini를 지원하는 한국어 AI 어시스턴트",
    "main": "dist/index.js",
    "bin": {
        "feelfree": "./dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/cli/index.ts",
        "start": "node dist/cli/index.js",
        "test": "jest",
        "test:unit": "jest --testPathPattern=tests/unit",
        "test:integration": "jest --testPathPattern=tests/integration",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "clean": "rimraf dist",
        "prepare": "npm run build"
    },
    "keywords": [
        "ai",
        "cli",
        "coding-assistant",
        "llama-cpp",
        "gemini",
        "korean",
        "한국어",
        "코딩어시스턴트",
        "repl",
        "chatbot"
    ],
    "author": "FeelFree AI Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dhsgud/feelfreeai-cli.git"
    },
    "bugs": {
        "url": "https://github.com/dhsgud/feelfreeai-cli/issues"
    },
    "homepage": "https://github.com/dhsgud/feelfreeai-cli#readme",
    "dependencies": {
        "@feelfreeai/core": "^0.5.0",
        "@types/figlet": "^1.7.0",
        "boxen": "^8.0.1",
        "chalk": "^4.1.2",
        "commander": "^12.1.0",
        "conf": "^10.2.0",
        "figlet": "^1.9.4",
        "gradient-string": "^3.0.0",
        "inquirer": "^8.2.6",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@types/inquirer": "^8.2.10",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.1",
        "jest": "^29.7.0",
        "prettier": "^3.4.1",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.2.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ]
}